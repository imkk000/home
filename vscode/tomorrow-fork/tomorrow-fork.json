  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#3a4248"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#8a9299"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#4a7a95",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#3a5a75"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#3a6a85"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#5a6a5f"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#a8955a"
      }
    },
    {
      "name": "Built-in constants",
      "scope": [
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#6a5a85"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#6a5a85"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#6a5a85"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#8a9299"
      }
    },
    {
      "name": "Tags",
      "scope": [
        "entity.name.tag",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#4a7a95"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#3a6a85"
      }
    },
    {
      "name": "Attribute IDs",
      "scope": [
        "entity.other.attribute-name.id",
        "punctuation.definition.entity"
      ],
      "settings": {
        "foreground": "#4a7a95"
      }
    },
    {
      "name": "Imports",
      "scope": [
        "entity.name.import.go"
      ],
      "settings": {
        "foreground": "#a85a57"
      }
    },
    {
      "name": "Go Package",
      "scope": [
        "keyword.package.go"
      ],
      "settings": {
        "foreground": "#a85a57",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Go Import",
      "scope": [
        "keyword.import.go"
      ],
      "settings": {
        "foreground": "#3a6a85"
      }
    },
    {
      "name": "Go Function",
      "scope": [
        "keyword.function.go"
      ],
      "settings": {
        "foreground": "#4a7a95",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Go Types",
      "scope": [
        "keyword.type.go"
      ],
      "settings": {
        "foreground": "#4a7a95",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": [
        "markup.bold",
        "punctuation.definition.bold"
      ],
      "settings": {
        "foreground": "#4a7a95",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": [
        "markup.italic",
        "punctuation.definition.italic"
      ],
      "settings": {
        "foreground": "#3a6a85",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Code",
      "scope": [
        "markup.inline.raw"
      ],
      "settings": {
        "foreground": "#5a6a5f"
      }
    },
    {
      "name": "Markup Link",
      "scope": [
        "markup.underline.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#3a6a85",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup List",
      "scope": [
        "markup.list"
      ],
      "settings": {
        "foreground": "#8a9299"
      }
    },
    {
      "name": "Markup Quote",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#5a6a5f",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#4a7a95"
      }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#3a6a85"
      }
    },
    {
      "name": "JSON Key - Level 2",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#3a5a75"
      }
    }
  ]
}